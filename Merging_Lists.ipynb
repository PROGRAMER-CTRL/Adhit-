      "source": [
        "def merge_and_remove_duplicates(list1, list2):\n",
        "    merged_list = list(set(list1 + list2))\n",
        "    return merged_list\n",
        "\n",
        "list1 = [100, 24, 74, 44]\n",
        "list2 = [44, 74, 95, 36]\n",
        "\n",
        "result = merge_and_remove_duplicates(list1, list2)\n",
        "\n",
        "print(\"Merged list without duplicates:\", result)\n"
      ]
    }
